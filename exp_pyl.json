{
    "batch_size": 50,
    "epochs": 100,
    "device": "cuda:0",
    "patience": 20,
    "classification": false,
    "dataset": {
      "root_dir": "data/graph_functions/",
      "format": "graph",
      "cache": "data/cache"
    },
    "train_dataset": {
      "mode": "pairs",
      "multiplier": 4,
      "split_file": "train.txt"
    },
    "val_dataset": {
      "mode": "singles",
      "split_subset": 50,
      "split_file": "val.txt"
    },
    "encoder_type": "gnn",
    "encoder": {
      "input_dim": 1,
      "node_embeddings": 5,
      "hidden_dim": 1024,
      "num_layers": 5
    },
    "optimizer": {
      "lr": 1e-3,
      "weight_decay": 0
    },
    "training": {
      "update_interval": 0.1
    },
    "val": {
    },
    "visualization": {
    }
  }
  